{
  "name": "refactionjs",
  "version": "1.0.0",
  "description": "Refactoring exercise for Node used as a test for potential JavaScript applicants",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel open:src",
    "open:src": "cross-env NODE_ENV=development parcel src/index.html --open",
    "test": "jest",
    "lint": "eslint src/ --ext .js --ext .jsx",
    "lint:watch": "npm run link -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xero-github/refactionjs"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-watch": "^5.1.2",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "prop-types": "^15.7.2",
    "react-hot-loader": "^4.8.4"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "express": "^4.15.3",
    "handlebars": "^4.0.10",
    "nedb": "^1.8.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1"
  },
  "babel": {
    "presets": ["env", "react"],
    "plugins": ["react-hot-loader/babel"]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "plugins": ["react"],
    "env": {
      "browser": true,
      "jest": true,
      "es6": true
    }
  }
}
